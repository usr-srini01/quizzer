							},
							"enableStaging": false,
							"validateDataConsistency": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['color']"
										},
										"sink": {
											"path": "$['color']"
										}
									},
									{
										"source": {
											"path": "$['value']"
